set confirm off
set architecture riscv:rv64
target remote 127.0.0.1:9877
symbol-file kernel-rv
set disassemble-next-line auto
set riscv use-compressed-breakpoints yes

layout split
b _start
b main

b ext4_bdif_bread
b ext4_bdif_unlock

set logging off
set logging file rv.log
set logging overwrite
set logging on

c
